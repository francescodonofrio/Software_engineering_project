<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" id="AnchorPane" prefHeight="542.0" prefWidth="863.0"
            xmlns="http://javafx.com/javafx/17" fx:controller="WindowController">
    <children>
        <VBox prefHeight="542.0" prefWidth="863.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
              AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <MenuBar>
                    <menus>
                        <Menu mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem mnemonicParsing="false" onAction="#saveWindow" text="Save"/>
                                <MenuItem mnemonicParsing="false" onAction="#loadWindow" text="Load"/>
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
                <HBox prefHeight="542.0" prefWidth="863.0">
                    <children>
                        <VBox prefHeight="542.0" prefWidth="180.0"
                              style="-fx-border-color: black; -fx-background-color: white;">
                            <children>
                                <Button mnemonicParsing="false" onAction="#lineSegmentSelection" prefHeight="27.0"
                                        prefWidth="867.0" style="-fx-border-color: black; -fx-background-color: white;"
                                        text="Line segment">
                                    <VBox.margin>
                                        <Insets top="10.0"/>
                                    </VBox.margin>
                                </Button>
                                <Button mnemonicParsing="false" onAction="#rectangleSelection" prefHeight="27.0"
                                        prefWidth="867.0" style="-fx-border-color: black; -fx-background-color: white;"
                                        text="Rectangle">
                                    <VBox.margin>
                                        <Insets top="10.0"/>
                                    </VBox.margin>
                                </Button>
                                <Button mnemonicParsing="false" onAction="#ellipseSelection" prefHeight="27.0"
                                        prefWidth="867.0" style="-fx-border-color: black; -fx-background-color: white;"
                                        text="Ellipse">
                                    <VBox.margin>
                                        <Insets top="10.0"/>
                                    </VBox.margin>
                                </Button>
                                <Label text="Choose internal color ">
                                    <VBox.margin>
                                        <Insets left="10.0" right="10.0" top="50.0"/>
                                    </VBox.margin>
                                </Label>
                                <ColorPicker fx:id="colorPickerInternal">
                                    <VBox.margin>
                                        <Insets left="10.0" right="10.0" top="10.0"/>
                                    </VBox.margin>
                                </ColorPicker>
                                <Label text="Choose contour color">
                                    <VBox.margin>
                                        <Insets left="10.0" right="10.0" top="20.0"/>
                                    </VBox.margin>
                                </Label>
                                <ColorPicker fx:id="colorPickerContour">
                                    <VBox.margin>
                                        <Insets left="10.0" right="10.0" top="10.0"/>
                                    </VBox.margin>
                                </ColorPicker>
                            </children>
                        </VBox>
                        <ScrollPane fx:id="scrollPane" prefHeight="432.0" prefWidth="684.0">
                            <content>
                                <AnchorPane fx:id="drawingPane" minHeight="0.0" minWidth="0.0"
                                            onMouseDragged="#DrawingWindowOnMouseDragged"
                                            onMousePressed="#DrawingWindowOnMousePressed"
                                            onMouseReleased="#DrawingWindowOnMouseReleased" prefHeight="516.0"
                                            prefWidth="682.0"/>
                            </content>
                        </ScrollPane>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</AnchorPane>
